{
	"name": "thetabox-gateway",
	"version": "0.0.1",
	"main": "lib/server.js",
	"typings": "lib/server.d.ts",
	"private": true,
	"license": "thetabox",
	"scripts": {
		"start": "npm run build && env-cmd -f dev.env node lib/server.js",
		"dev": "kill-port 4444 && env-cmd -f dev.env nodemon",
		"test": "jest --watchAll --forceExit --detectOpenHandles",
		"build": "tsc -p .",
		"build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
		"build:export": "nodemon",
		"update": "ncu -u @theta*/*",
		"docker": "docker build -t thetabox/thetabox-gateway:0.0.1 -f ./Dockerfile . --label 'fix=init'",
		"run": "docker run --rm -it  thetabox/thetabox-gateway:0.0.1",
		"push": "docker push thetabox/thetabox-gateway:0.0.1",
		"deploy": "npm run build && npm run docker && npm run push"
	},
	"dependencies": {
		"@google-cloud/logging-bunyan": "^3.3.0",
		"@thetabox/model": "^0.0.39",
		"@thetabox/services": "^0.0.16",
		"@types/bunyan": "^1.8.8",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/express-fileupload": "^1.2.2",
		"@types/express-http-proxy": "^1.6.3",
		"@types/fs-extra": "^9.0.13",
		"@types/hpp": "^0.2.2",
		"@types/node": "^17.0.23",
		"bunyan": "^1.8.15",
		"cors": "^2.8.5",
		"delay": "^5.0.0",
		"dotenv": "^16.0.0",
		"env-cmd": "^10.1.0",
		"express": "^4.17.3",
		"express-fileupload": "^1.3.1",
		"express-http-proxy": "^1.6.3",
		"fs-extra": "^10.0.1",
		"helmet": "^5.0.2",
		"hpp": "^0.2.3",
		"typescript": "^4.6.3"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/supertest": "^2.0.12",
		"jest": "^27.5.1",
		"jest-watch-typeahead": "^1.0.0",
		"kill-port": "^1.6.1",
		"nodemon": "^2.0.15",
		"npm-check-updates": "^12.5.8",
		"prettier": "^2.6.2",
		"supertest": "^6.2.2",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0"
	}
}